APP_NAME="Video Downloader"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
DB_DATABASE=database/database.sqlite

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Video Downloader Configuration
# ================================

# External API Configuration (RapidAPI)
RAPIDAPI_KEY=your_rapidapi_key_here
RAPIDAPI_HOST=youtube-mp36.p.rapidapi.com
DOWNLOADER_USE_EXTERNAL_API=true
DOWNLOADER_API_TIMEOUT=30

# yt-dlp Configuration (optional)
DOWNLOADER_USE_YTDLP=false
YTDLP_BINARY_PATH=yt-dlp
YTDLP_TIMEOUT=60
YTDLP_MAX_FILESIZE=500M

# Direct Parsing Configuration
DOWNLOADER_USE_DIRECT_PARSING=true
DOWNLOADER_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36"
DOWNLOADER_DIRECT_TIMEOUT=30
DOWNLOADER_VERIFY_SSL=false

# File Storage Configuration
DOWNLOADER_STORAGE_DISK=public
DOWNLOADER_STORAGE_DIRECTORY=downloads
DOWNLOADER_MAX_FILESIZE=104857600
DOWNLOADER_CLEANUP_HOURS=24

# Queue Configuration
DOWNLOADER_QUEUE_CONNECTION=database
DOWNLOADER_QUEUE_NAME=downloads
DOWNLOADER_QUEUE_TIMEOUT=300
DOWNLOADER_MAX_TRIES=3
DOWNLOADER_RETRY_DELAY=60

# Rate Limiting Configuration
DOWNLOADER_RATE_LIMITING=true
DOWNLOADER_MAX_REQUESTS_PER_MINUTE=10
DOWNLOADER_MAX_REQUESTS_PER_HOUR=100
DOWNLOADER_MAX_CONCURRENT=3

# Security Configuration
DOWNLOADER_ALLOWED_DOMAINS=
DOWNLOADER_BLOCKED_DOMAINS=
DOWNLOADER_MAX_URL_LENGTH=2048
DOWNLOADER_SANITIZE_FILENAMES=true

# Logging Configuration
DOWNLOADER_LOGGING=true
DOWNLOADER_LOG_CHANNEL=daily
DOWNLOADER_LOG_SUCCESS=true
DOWNLOADER_LOG_FAILURES=true
DOWNLOADER_LOG_USER_AGENTS=false
DOWNLOADER_LOG_IPS=false

# Cache Configuration
DOWNLOADER_CACHE_ENABLED=true
DOWNLOADER_CACHE_TTL=3600
DOWNLOADER_CACHE_PREFIX=downloader
DOWNLOADER_CACHE_STORE=default

# Feature Flags
DOWNLOADER_BACKGROUND_PROCESSING=true
DOWNLOADER_DOWNLOAD_HISTORY=true
DOWNLOADER_THUMBNAILS=true
DOWNLOADER_FORMAT_SELECTION=true
DOWNLOADER_QUALITY_SELECTION=true
DOWNLOADER_BATCH_DOWNLOADS=false
DOWNLOADER_PLAYLIST_SUPPORT=false
DOWNLOADER_SUBTITLES=false